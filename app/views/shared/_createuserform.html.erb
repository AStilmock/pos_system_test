<%= form_with url: "/login", method: :post, local: true do |f| %>
  <%= f.label  :username %>
  <%= f.text_field :username %>

  <%= f.label  :email %>
  <%= f.text_field :email %>

  <%= f.label  :password %>
  <%= f.text_field :password %>

  <%= f.label  :confirm_password %>
  <%= f.text_field :confirm_password %>

  <%= f.submit "Create User" %>
<% end %>