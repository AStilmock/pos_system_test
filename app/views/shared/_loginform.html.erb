<%= form_with url: user_shopping_path, method: :get, local: true do |f| %>
  <%= f.label  :username %>
  <%= f.text_field :username %>

  <%= f.label  :password %>
  <%= f.text_field :password %>

  <%= f.label  :confirm_password %>
  <%= f.text_field :confirm_password %>

  <%= f.submit "Login" %>
<% end %>